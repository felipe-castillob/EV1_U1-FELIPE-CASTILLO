@model IEnumerable<EV1_U1_Felipe_Castillo.Models.Servicio>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.RutCliente)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DescServicio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PrecioServicio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaInicioServicio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaTerminoServicio)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.RutCliente)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DescServicio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PrecioServicio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaInicioServicio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaTerminoServicio)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdServicio">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdServicio">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdServicio">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
